{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
    "urlEndPoint": "https://ep-smilesoft-develop.azurewebsites.net",
    "TimeOutSession": 60,
    "Environment": "Dev"
  },
  "Identity": {
    "Password": {
      "RequireDigit": true,
      "RequiredLength": 8,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": true,
      "RequireLowercase": false
    },
    "Lockout": {
      "DefaultLockoutTimeSpan": "0:05:00",
      "MaxFailedAccessAttempts": 5
    }
  }
}